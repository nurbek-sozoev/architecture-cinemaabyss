    "koa-json": "^2.0.2",
    "koa-cors": "^0.0.16",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/koa": "^2.13.12",
    "@types/koa-router": "^7.4.8",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-logger": "^3.1.5",
    "@types/koa-json": "^2.0.23",
    "@types/koa-cors": "^0.0.5",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
