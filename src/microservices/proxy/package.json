{
  "name": "cinemaabyss-proxy",
  "version": "1.0.0",
  "description": "Strangler Fig API Gateway for CinemaAbyss - Node.js + TypeScript + Koa",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
